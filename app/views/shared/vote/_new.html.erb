<%= form_for parent.votes.build(value: one) do |f| %>
  <%= f.hidden_field :voteable_type %>
  <%= f.hidden_field :voteable_id %>
  <%= f.hidden_field :value %>
  <%= f.button type: 'submit', class: "my-button clickable", id: vote_type, disabled: parent.users_vote_on_post_exists?(current_user) do %>
    <%= tag.i class: "fa fa-chevron-circle-#{vote_type}" %>
  <% end %>
<% end %>
