<%= form_for(parent.votes.build(value: one), url: votes_path, action: "create") do |f| %>
  <%= f.hidden_field :voteable_type, value: parent.class %>
  <%= f.hidden_field :voteable_id, value: parent.id %>
  <%= f.hidden_field :value, value: one %>
  <%= f.button type: 'submit', class: "my-button clickable", id: vote_type do %>
    <%= tag.i class: "fa fa-chevron-circle-#{vote_type}" %>
  <% end %>
<% end %>
