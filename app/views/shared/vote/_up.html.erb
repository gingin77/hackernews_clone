<%= form_for parent.votes.find_or_initialize_by(user_id: current_user.id) do |f| %>
  <%= f.hidden_field :voteable_type %>
  <%= f.hidden_field :voteable_id %>
  <%= f.hidden_field :value, value: 1 %>
  <%= f.button type: 'submit', class: "my-button clickable", id: "up" do %>
    <%= tag.i class: "fa fa-chevron-circle-up" %>
  <% end %>
<% end %>
