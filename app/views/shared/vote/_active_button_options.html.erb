<% if !user_signed_in? %>
  <%= render partial: "shared/vote/static_icons", locals: { parent: parent } %>
<% else %>
  <%= render partial: "shared/vote/inactive_active_together",
    locals: {
              parent: parent,
              vote: parent.users_vote(current_user),
              one: 1,
              b_vote_type: "up"
            } %>
  <%= render partial: "shared/vote/inactive_active_together",
    locals: {
              parent: parent,
              vote: parent.users_vote(current_user),
              one: -1,
              b_vote_type: "down"
            } %>
<% end %>
